  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod && ng run amitport:server:production",
    "test": "npm run prerender && npm run serve:test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "prerender": "ng run amitport:prerender --routes=/",
    "serve:test": "concurrently \"npm run serve:ssr\" \"npm run e2e\" --kill-others --success first",
    "serve:ssr": "node dist/amitport/server/main.js",
    "serve:ssr:dev": "ng run amitport:serve-ssr:production",
    "build:ssr": "npm run build:client-and-server-bundles",
    "publish": "ng deploy --no-build --branch=master"
  },
